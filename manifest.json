{
  "manifest_version": 2,
  "name": "MiSkip",
  "description": "YouTube Ad Skipper",
  "version": "2.0",
  
  "content_scripts": [
    {
    "js": ["script.js"],
    "run_at": "document_idle",
    "matches":["<all_urls>"]
        
        
    }],
  
    "permissions":[
      "tabs",
      "activeTab"
    ]
}
